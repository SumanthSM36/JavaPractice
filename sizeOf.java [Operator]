public class sizeOf {
	public static void main(String[] args) {

		System.out.println("sizeOf an Byte DataType is "+ (Byte.SIZE/8));       //.BYTES
		System.out.println("sizeOf an Short DataType is "+ (Short.SIZE/8));
		System.out.println("sizeOf an Integer DataType is "+ (Integer.SIZE/8));
		System.out.println("sizeOf an Float DataType is "+ (Float.BYTES));
		System.out.println("sizeOf an Double DataType is "+ (Double.BYTES));
		System.out.println("sizeOf an Character DataType is "+ (Character.BYTES));
		System.out.println("sizeOf an Long DataType is "+ (Long.SIZE/8));
	}
}
